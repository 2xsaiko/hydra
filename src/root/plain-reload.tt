[% WRAPPER layout.tt title=title %]
[% PROCESS common.tt %]

[% project = build.project %]
[% jobset = build.jobset %]
[% job = build.job %]

<h2>Last 50 lines of log</h2>

[% IF reload %]
<script>
 $(document).ready(function() {
     $("#contents").load("[% url %]");
   var refreshId = setInterval(function() {
      $("#contents").load("[% url %]");
   }, 5000);
});
</script>
[% END %]

<pre class="taillog" id="contents">
[% contents -%]
</pre>

[% END %]
